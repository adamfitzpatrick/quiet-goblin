{
  "dev": {
    "port": 7002,
    "log_transports": [
      {
        "type": "Console",
        "params": {
          "name": "console",
          "colorize": true,
          "timestamp": true
        }
      }
    ],
    "static_source": "../raging-goblin/public",
    "dynamo_tables": {
      "posts": "ragingGoblin_qa_posts"
    }
  },
  "e2e": {
    "port": 7003,
    "log_transports": [
      {
        "type": "Console",
        "params": {
          "name": "console",
          "colorize": true,
          "timestamp": true
        }
      }
    ],
    "static_source": "../raging-goblin/public",
    "dynamo_tables": {
      "posts": "ragingGoblin_qa_posts"
    }
  },
  "qa": {
    "log_transports": [
      { "type": "Console", "params": { "name": "console", "colorize": true }}
    ],
    "static_source": "./public",
    "dynamo_tables": {
      "posts": "ragingGoblin_qa_posts"
    }
  },
  "prod": {
    "log_transports": [
      { "type": "File", "params": { "filename": "./logs/quiet-goblin.log", "maxsize": 1000000 }}
    ],
    "static_source": "/public",
    "dynamo_tables": {
      "posts": "ragingGoblin_posts"
    }
  }
}
