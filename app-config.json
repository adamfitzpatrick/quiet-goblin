{
  "dev": {
    "port": 7002,
    "domain": "http://127.0.0.1:7002",
    "log_transports": [
      {
        "type": "Console",
        "params": {
          "name": "console",
          "colorize": true,
          "timestamp": true
        }
      }
    ],
    "static_source": "../raging-goblin/public",
    "dynamo_tables": {
      "posts": "ragingGoblin_qa_posts",
      "user": "ragingGoblin_qa_user",
      "comment": "ragingGoblin_qa_comment"
    },
    "secret": "./token-rsa.key"
  },
  "e2e": {
    "port": 7003,
    "domain": "http://www.stepinto.io",
    "log_transports": [
      {
        "type": "Console",
        "params": {
          "name": "console",
          "colorize": true,
          "timestamp": true
        }
      }
    ],
    "static_source": "../raging-goblin/public",
    "dynamo_tables": {
      "posts": "ragingGoblin_qa_posts",
      "user": "ragingGoblin_qa_user",
      "comment": "ragingGoblin_qa_comment"
    },
    "secret": "./token-rsa.key"
  },
  "qa": {
    "domain": ".stepinto.io",
    "log_transports": [
      { "type": "Console", "params": { "name": "console", "colorize": true }}
    ],
    "static_source": "./public",
    "dynamo_tables": {
      "posts": "ragingGoblin_qa_posts",
      "user": "ragingGoblin_qa_user",
      "comment": "ragingGoblin_qa_comment"
    },
    "secret": "./token-rsa.key"
  },
  "prod": {
    "domain": ".stepinto.io",
    "log_transports": [
      { "type": "File", "params": { "filename": "./logs/quiet-goblin.log", "maxsize": 1000000 }}
    ],
    "static_source": "./public",
    "dynamo_tables": {
      "posts": "ragingGoblin_posts",
      "user": "ragingGoblin_user",
      "comment": "ragingGoblin_comment"
    },
    "secret": "./token-rsa.key"
  }
}
