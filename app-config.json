{
  "dev": {
    "port": 7002,
    "log_transports": [
      { "type": "Console", "params": { "name": "console", "colorize": true } }
    ],
    "static_source": "../raging-goblin/public",
    "dynamo_endpoint": "http://localhost:8000"
  },
  "e2e": {
    "port": 7003,
    "log_transports": [
      { "type": "Console", "params": { "name": "console", "colorize": true } }
    ],
    "static_source": ".",
    "dynamo_endpoint": "http://localhost:8000"
  },
  "prod": {
    "port": 8080,
    "log_transports": [
      { "type": "File", "params": { "filename": "./logs/quiet-goblin.log" }}
    ],
    "static_source": "/public",
    "dynamo_endpoint": "http://localhost:8000"
  }
}