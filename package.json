{
  "name": "quiet-goblin",
  "version": "0.0.1",
  "description": "Backend implementation for raging-goblin",
  "main": "app.js",
  "scripts": {
    "ci": "./start-ci.sh",
    "cucumber": "cucumber.js ./e2e/features --require ./e2e/support",
    "e2e": "cucumber.js ./e2e/features --require ./e2e/support --tags ~@ignore",
    "gulp": "gulp",
    "keygen": "openssl genrsa -out token-rsa.key 1024",
    "lint": "jshint . --exclude-path=.gitignore --reporter=node_modules/jshint-stylish ",
    "manage": "node ./utility/utilities.js",
    "start": "npm run keygen && node app.js",
    "tdd": "gulp tdd",
    "test": "gulp test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/adamfitzpatrick/quiet-goblin.git"
  },
  "author": "Adam Fitzpatrick",
