{
  "name": "quiet-goblin",
  "version": "0.1.0",
  "description": "Backend implementation for raging-goblin",
  "main": "app.js",
  "scripts": {
    "ci": "npm test && istanbul check-coverage",
    "e2e": "istanbul cover _mocha $(find e2e -name '*.spec.js') -- -R nyan",
    "e2e-tdd": "watch 'npm run -s e2e' ./**/ --filter=./spec-filter.js --interval=1",
    "lint": "jshint . --exclude-path=.gitignore --reporter=node_modules/jshint-stylish ",
    "manage": "node ./utility/utilities.js",
    "start": "node app.js",
    "tdd": "watch 'npm test' ./src/**/ --filter=./spec-filter.js --interval=0.1",
    "test": "npm run -s lint && istanbul cover _mocha $(find src -name '*.spec.js') -- -R nyan"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/adamfitzpatrick/quiet-goblin.git"
  },
  "author": "Adam Fitzpatrick",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/adamfitzpatrick/quiet-goblin/issues"
  },
  "homepage": "https://github.com/adamfitzpatrick/quiet-goblin#readme",
  "dependencies": {
    "aws-sdk": "^2.6.2",
    "body-parser": "^1.15.2",
    "express": "^4.14.0",
    "mkdirp": "^0.5.1",
    "uuid": "^2.0.3",
    "yargs": "^5.0.0"
  },
  "devDependencies": {
    "bluebird": "^3.4.6",
    "chai": "^3.5.0",
    "chai-as-promised": "^5.3.0",
    "chai-datetime": "^1.4.1",
    "chalk": "^1.1.3",
    "faker": "^3.1.0",
    "inquirer": "^1.1.3",
    "istanbul": "^0.4.5",
    "jshint": "^2.9.3",
    "jshint-stylish": "^2.2.1",
    "mocha": "^3.0.2",
    "ping": "^0.2.0",
    "prompt": "^1.0.0",
    "rewire": "^2.5.2",
    "rimraf": "^2.5.4",
    "sinon": "^1.17.5",
    "watch": "^0.19.2",
    "winston": "^2.2.0"
  }
}
