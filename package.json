  "bugs": {
    "url": "https://github.com/adamfitzpatrick/quiet-goblin/issues"
  },
  "homepage": "https://github.com/adamfitzpatrick/quiet-goblin#readme",
  "dependencies": {
    "aws-sdk": "^2.6.2",
    "bcrypt": "^0.8.7",
    "bluebird": "^3.4.6",
    "body-parser": "^1.15.2",
    "express": "^4.14.0",
    "github": "^6.0.1",
    "helmet": "^3.0.0",
    "jsonwebtoken": "^7.1.9",
    "mkdirp": "^0.5.1",
    "multer": "^1.2.0",
    "request": "^2.75.0",
    "uuid": "^2.0.3",
    "winston": "^2.2.0",
    "yargs": "^6.0.0"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "chai-as-promised": "^6.0.0",
    "chalk": "1.1.3",
    "cucumber": "^1.3.1",
    "faker": "^3.1.0",
    "gulp": "^3.9.1",
    "gulp-debug": "^2.1.2",
    "gulp-filter": "^4.0.0",
    "gulp-if": "^2.0.1",
    "gulp-jshint": "^2.0.1",
    "gulp-plumber": "^1.1.0",
    "gulp-spawn-mocha": "^3.1.0",
    "gulp-util": "^3.0.7",
    "inquirer": "^1.1.3",
    "istanbul": "^0.4.5",
    "jshint": "^2.9.3",
    "jshint-stylish": "^2.2.1",
    "mocha": "^3.0.2",
    "rewire": "^2.5.2",
